@model IEnumerable<MVC_Assessment.Models.Customer>
@{
    ViewBag.Title = "Customers";
}

<h2>Customers In Germany</h2>
<table border="1" class="table">
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Contact Name</th>
            <th>City</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.ContactName</td>
    <td>@customer.City</td>
    <td>@customer.Phone</td>
</tr>}
    </tbody>
</table>

