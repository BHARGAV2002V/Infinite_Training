
@{
    ViewBag.Title = "CustomerOrder";
}

<h2>Customer Order Details</h2>
<table border="1" class="table">
    <tr>
        <th>Customer ID</th>
        <td>@Model.CustomerID</td>
    </tr>
    <tr>
        <th>Contact Name</th>
        <td>@Model.ContactName</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>City</th>
        <td>@Model.City</td>
    </tr>
    <tr>
        <th>Country</th>
        <td>@Model.Country</td>
    </tr>
    <tr>
        <th>Phone</th>
        <td>@Model.Phone</td>
    </tr>
</table>


